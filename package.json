{
  "name": "@owpz/prisma-ksuid",
  "version": "25.9.20",
  "description": "Prisma Client extension for generating K-Sortable Unique IDs (KSUIDs)",
  "author": "Apex Innovations, Inc.",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "test": "npm run prisma:generate && jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest __tests__/extension-e2e.test.ts",
    "test:fuzz": "jest __tests__/fuzz.test.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma db push --force-reset"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=4.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@prisma/client": "6.16.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.0.0",
    "eslint": "^9.25.1",
    "fast-check": "4.3.0",
    "jest": "^29.7.0",
    "prisma": "6.16.2",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "keywords": [
    "prisma",
    "extension",
    "ksuid",
    "id",
    "uuid",
    "sortable",
    "prisma-extension"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/owpz/prisma-ksuid.git"
  },
  "bugs": {
    "url": "https://github.com/owpz/prisma-ksuid/issues"
  },
  "homepage": "https://github.com/owpz/prisma-ksuid",
  "eslint": {
    "configFile": "eslint.config.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@owpz/ksuid": "^25.7.20"
  }
}
